{
  "name": "comcrm",
  "version": "0.0.1",
  "description": "Comerical CRM",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && swc ./src -d dist",
    "start:build": "npm run build && node --inspect=0.0.0.0 dist/index.js",
    "dev": "nodemon -w ./src --ext \"ts\" --exec \"npm run start:build\"",
    "start": "ts-node src/index.ts",
    "codegen": "graphql-codegen"
  },
  "author": "David Mitchell <thecodingtree>",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.82",
    "@tsconfig/node18": "^18.2.1",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.5",
    "@types/node": "^18.17.14",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "@prisma/client": "^5.2.0",
    "apollo-server-express": "^3.12.1",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "graphql-scalars": "^1.22.2",
    "graphql-tag": "^2.12.6",
    "morgan": "^1.10.0"
  }
}
